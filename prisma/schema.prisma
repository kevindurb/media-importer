generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model ImportFile {
  id Int @id @default(autoincrement())
  path String @unique
  tmdbMatchId Int?
  isTVShow Boolean @default(false)
  updatedAt DateTime @updatedAt
  createdAt DateTime @default(now())
}
