generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model ImportFile {
  id Int @id @default(autoincrement())
  path String @unique
  tmdbMatchId Int?
  isTVShow Boolean @default(false)
  season Int?
  episode Int?
  year Int?
  importPath String?
  updatedAt DateTime @updatedAt @default(now())
  createdAt DateTime @default(now())
}
